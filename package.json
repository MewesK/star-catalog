{
  "name": "star-catalog",
  "version": "1.1.0",
  "description": "Explore the HYG star database visually.",
  "main": "./out/main/index.js",
  "author": "MewesK",
  "homepage": "https://github.com/MewesK/star-catalog",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts,.vue --fix",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "vue-tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "start": "electron-vite preview",
    "dev": "electron-vite dev",
    "build": "npm run typecheck && electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:win": "npm run build && electron-builder --win --config",
    "build:mac": "npm run build && electron-builder --mac --config",
    "build:linux": "npm run build && electron-builder --linux --config",
    "build:icons": "electron-icon-builder --flatten --input=./resources/logo.png --output=./build && mv ./build/icons/icon.* ./build && cp ./build/icons/512x512.png ./build/icon.png"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^2.0.0",
    "@electron-toolkit/utils": "^2.0.0"
  },
  "devDependencies": {
    "@babylonjs/core": "^6.19.1",
    "@babylonjs/gui": "^6.19.1",
    "@babylonjs/inspector": "^6.19.1",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@electron/notarize": "^2.1.0",
    "@rushstack/eslint-patch": "^1.3.2",
    "@types/node": "^18.16.19",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vueuse/core": "^10.3.0",
    "csv-parse": "^5.4.0",
    "electron": "^25.4.0",
    "electron-builder": "^24.6.3",
    "electron-icon-builder": "^2.0.1",
    "electron-vite": "^1.0.27",
    "eslint": "^8.46.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-vue": "^9.16.1",
    "material-design-icons-iconfont": "^6.7.0",
    "prettier": "^3.0.1",
    "typescript": "^5.1.6",
    "vite": "^4.4.2",
    "vite-plugin-vuetify": "^1.0.2",
    "vue": "^3.3.4",
    "vue-tsc": "^1.8.4",
    "vuetify": "^3.3.11"
  }
}
